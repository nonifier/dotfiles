# Nonifier github
Host github.com
    User git
    AddKeysToAgent yes
    IdentitiesOnly yes
    Match exec "bash -c '[[ $PWD =~ '-store\|dotfiles' ]]'"
    IdentityFile ~/.ssh/nonifier/id_rsa 
    LogLevel INFO

Host *
    AddKeysToAgent yes
    IdentitiesOnly yes
    IdentityFile ~/.ssh/prophesee/id_ed25519 
    LogLevel INFO

Host imx8mqthor96
    Hostname imx8mqthor96.local
    User root
    IdentityFile ~/.ssh/nonifier/id_rsa
    LocalForward 1234 localhost:1234

